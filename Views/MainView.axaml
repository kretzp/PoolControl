<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:pages="clr-namespace:PoolControl.Pages;assembly=PoolControl"
			 xmlns:p="clr-namespace:PoolControl"
			 Width="480" Height="320" Design.Width="480" Design.Height="320"
			 x:Class="PoolControl.Views.MainView"
			 FontSize="12">
	<Panel>
		<TabControl x:Name="TabControl" Classes="sidebar">
			<TabItem Header="{x:Static p:Resource.Overview}">
				<pages:Overview />
			</TabItem>
			<TabItem Header="{x:Static p:Resource.FilterPump}">
				<pages:FilterPump />
			</TabItem>
			<TabItem Header="{x:Static p:Resource.SolarHeater}">
				<pages:SolarHeater />
			</TabItem>
			<TabItem Header="{x:Static p:Resource.PhManagement}">
				<pages:PhValue />
			</TabItem>
			<TabItem Header="{x:Static p:Resource.RedoxManagement}">
				<pages:Redox />
			</TabItem>
			<TabItem Header="{x:Static p:Resource.Cistern}">
				<pages:Cistern />
			</TabItem>
		</TabControl>
	</Panel>
</UserControl>
